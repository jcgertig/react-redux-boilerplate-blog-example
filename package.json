{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "react template with redux",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.js",
    "build": "npm run clean && npm run build:webpack && cross-env NODE_ENV=production npm run server",
    "heroku": "npm run server",
    "heroku-postbuild": "webpack -p --config ./config/webpack.config.js --progress",
    "start": "cross-env NODE_ENV=development npm run server",
    "dev": "npm run start",
    "prod": "npm run build",
    "staging": "cross-env NODE_ENV=staging npm run server",
    "server": "node server.js"
  },
  "author": "Jonathan Gertig <jcgertig@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "body-parser": "^1.16.1",
    "classnames": "^2.2.5",
    "color-it": "^1.2.7",
    "compression": "^1.6.2",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "emojic": "^1.1.7",
    "express": "^4.14.1",
    "fetchum": "^3.2.0",
    "fetchum-redux": "^1.0.1",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template": "git://github.com/jcgertig/html-webpack-template",
    "json-loader": "^0.5.4",
    "lodash.assign": "^4.2.0",
    "lodash.find": "^4.6.0",
    "lodash.isequal": "^4.5.0",
    "postcss-loader": "^1.2.2",
    "precss": "^1.4.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.2",
    "react-router-dom": "^4.0.0-beta.3",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sequelize": "^3.30.2",
    "sqlite3": "^3.1.8",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-merge": "^2.6.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^1.0.1",
    "webpack-dev-server": "^2.2.1",
    "webpack-hot-middleware": "^2.16.1"
  }
}
